@model IEnumerable<Order>

@if (Model.Count() > 0)
{
    @foreach(Order ord in Model)
    {
        <div class="my-3">
            <partial name="_OrderDetails" model="@ord">
        </div>
    }

}
else
{
    <div class="alert alert-info">
        <p class="lead">There is no order yet</p>
    </div>
}
